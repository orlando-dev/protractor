{
  "name": "protractor",
  "version": "1.0.0",
  "description": "Projeto para o curso de Protractor da Escola TAT",
  "main": "index.js",
  "scripts": {
    "lint": "npx standard",
    "lint:fix": "npx standard --fix",
    "pretest:github": "webdriver-manager update --version.chrome 89.0.4389.23",
    "pretest:local": "webdriver-manager update",
    "test": "npm run test:chrome && npm run test:firefox",
    "test:chrome": "protractor configurations/chromeConfig.js",
    "test:firefox": "protractor configurations/firefoxConfig.js"
  },
  "standard": {
    "globals": [
      "beforeAll",
      "beforeEach",
      "BhxBrowser",
      "by",
      "describe",
      "element",
      "expect",
      "it"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://orlando-junior@bitbucket.org/orlando-junior/protractor.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/orlando-junior/protractor#readme",
  "devDependencies": {
    "faker": "^5.5.2",
    "protractor": "^7.0.0",
    "protractor-helper": "^4.2.1",
    "standardjs": "^1.0.0-alpha"
  }
}
